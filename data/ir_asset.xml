    <field name="name">Primary variables SCSS</field>
    <field name="bundle">web._assets_primary_variables</field>
    <field name="path">theme_now_v2/static/src/scss/primary_variables.scss</field>
</record>

<record id="theme_now_v2.bootstrap_overridden_scss" model="theme.ir.asset">
